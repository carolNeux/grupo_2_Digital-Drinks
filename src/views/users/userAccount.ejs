<!DOCTYPE html>
<html lang="en">
<head>
    <!--head/partial-->
    <%- include('../partials/head') %>
    <!--fin head/partial-->
    <link rel="stylesheet" href="/stylesheets/usersList.css">
</head>
<body>
    <!--header/partial-->
    <%- include('../partials/header') %>
    <!--fin header/partial--> 
    <section class="user_main_container">
        <h2>Detalle de usuario:</h2>
            <article class="user_list_container">
                <div><h4>Nombre: </h4><p> <%= userInformation.first_name %></p></div>
                <div><h4>Apellido: </h4><p> <%= userInformation.last_name %></p></div>
                <div><h4>Nombre de Usuario: </h4><p> <%= userInformation.username %></p></div>
                <div><h4>Email:</h4><p> <%= userInformation.email %></p></div>
                <div><h4>Fecha de nacimiento: </h4><p> <%= userInformation.birthday %></p></div>
                <div><h4>Contrase√±a: </h4><p> ********</p></div>
                <div class="user_buttons_container">
                    <div class="user_button_back">
                        <a href="/">Volver al Home</a>
                    </div>
                    <form action="/users/accountEdit/<%= userInformation.id %>" method="GET">
                        <input type="submit" value="Editar">
                    </form>
                </div>
            </article>
    </section>
    <!--footer/partial-->
    <%- include('../partials/footer') %>
    <!--final footer/partial-->        
</body>
</html>